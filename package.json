{
  "name": "ani2mal",
  "version": "1.0.0",
  "description": "Sync your Anilist anime list to MyAnimeList",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node server.js",
    "dev": "nodemon --inspect server.js",
    "exec": "node server.js --exec babel-node --presets es2015,stage-2",
    "devtest": "jest --watch --runInBand",
    "test": "--verbose --bail --forceExit",
    "coverage": "jest --coverage",
    "build": "webpack",
    "devbuild": "webpack --progress --watch",
    "deploy": "now -C --docker -t $NOW_API_TOKEN"
  },
  "engines": {
    "node": ">=7.10.0"
  },
  "now": {
    "dotenv": "variables.env.now"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gilmoreg/anlist-to-mal.git"
  },
  "author": "Grayson Gilmore (gilmoreg@live.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gilmoreg/anlist-to-mal/issues"
  },
  "homepage": "https://github.com/gilmoreg/anlist-to-mal#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-watch": "2.0.6",
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "css-loader": "0.28.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.1.3",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jquery": "1.2.0",
    "extract-text-webpack-plugin": "2.1.0",
    "fetch-mock": "5.10.0",
    "jest": "20.0.1",
    "node-mocks-http": "1.6.2",
    "now": "6.1.1",
    "style-loader": "0.17.0",
    "webpack": "2.5.1"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.17.1",
    "btoa": "1.1.2",
    "compression": "1.6.2",
    "connect-mongo": "1.3.2",
    "cors": "2.8.3",
    "dotenv": "4.0.0",
    "es6-promisify": "5.0.0",
    "express": "4.15.2",
    "express-session": "1.15.2",
    "express-validator": "3.2.0",
    "koa": "2.2.0",
    "limiter": "1.1.0",
    "mongoose": "4.9.8",
    "mongoose-mongodb-errors": "0.0.2",
    "node-fetch": "1.6.3",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "passport-local-mongoose": "4.0.0",
    "pug": "2.0.0-rc.1",
    "validator": "7.0.0",
    "xml2js": "0.4.17"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ]
  }
}
